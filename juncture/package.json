{
  "name": "ezpage",
  "version": "0.3.24",
  "private": false,
  "scripts": {
    "dev": "vite",
    "build": "tailwindcss -i ./src/input.css -o ./src/tailwind.css && run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build --base=/ezpage-wc/",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "tailwind": "tailwindcss -i ./src/input.css -o ./src/tailwind.css --watch"
  },
  "dependencies": {
    "js-sha256": "^0.10.1",
    "jwt-decode": "^3.1.2",
    "marked": "^7.0.4",
    "netlify-identity-widget": "^1.9.2",
    "openseadragon": "^4.0.0",
    "preline": "^1.9.0",
    "ts-md5": "^1.3.1",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^18.17.0",
    "@types/openseadragon": "^3.0.6",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/tsconfig": "0.4.0",
    "autoprefixer": "^10.4.15",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.28",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.1.6",
    "vite": "^4.4.6",
    "vue-tsc": "^1.8.6"
  }
}
